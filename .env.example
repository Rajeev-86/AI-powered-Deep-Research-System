# Environment Variables for Production Deployment
# Copy this file to .env for local testing, or set these in your hosting platform

# Enable environment variable configuration (set to "true" in production)
USE_ENV_CONFIG=true

# GitHub Token for GPT-5/GPT-4o access via GitHub Models
GITHUB_TOKEN=ghp_your_github_token_here

# Gemini API Keys (JSON array format)
# Example: ["key1", "key2", "key3"]
GEMINI_API_KEYS=["AIzaSyXXXXXXXXXXXXXXXXXXXXXX","AIzaSyYYYYYYYYYYYYYYYYYYYYYY"]

# Google Custom Search API Keys (JSON array format)
GOOGLE_SEARCH_KEYS=["YOUR_GOOGLE_KEY_1","YOUR_GOOGLE_KEY_2"]
GOOGLE_SEARCH_ENGINE_IDS=["YOUR_CX_ID_1","YOUR_CX_ID_2"]

# Tavily Search API Key
TAVILY_API_KEY=tvly-XXXXXXXXXXXXXXXX
